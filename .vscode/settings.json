{
  "[json]": {
    "prettier.printWidth": 0
  },
  "[jsonc]": {
    "prettier.printWidth": 0
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "json.schemas": [
    {
      "fileMatch": [
        "completions/*/language/*.json"
      ],
      "url": "/schema/completion-manifest.zh-CN.json"
    },
    {
      "fileMatch": [
        "completions/*/config.json"
      ],
      "url": "/schema/completion-config.zh-CN.json"
    }
  ]
}
