{
  "[json]": {
    "prettier.printWidth": 0
  },
  "[jsonc]": {
    "prettier.printWidth": 0
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "json.schemas": [
    {
      "fileMatch": [
        "completions/**/config.json"
      ],
      "url": "/schema/zh-CN/config.json"
    },
    {
      "fileMatch": [
        "completions/**/language/*.json"
      ],
      "url": "/schema/zh-CN/completion.json"
    }
  ]
}
