{
  "[json]": {
    "prettier.printWidth": 0
  },
  "[jsonc]": {
    "prettier.printWidth": 0
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "json-schema-plus.schemas": [
    {
      "fileMatch": [
        "completions/*/language/*.json"
      ],
      "urls": [
        {
          "language": "zh",
          "url": "/schema/completion-manifest.zh-CN.json"
        },
        {
          "language": "en",
          "url": "/schema/completion-manifest.en-US.json"
        }
      ],
      "url": "/schema/completion-manifest.en-US.json"
    },
    {
      "fileMatch": [
        "completions/*/config.json"
      ],
      "urls": [
        {
          "language": "zh",
          "url": "/schema/completion-config.zh-CN.json"
        },
        {
          "language": "en",
          "url": "/schema/completion-config.en-US.json"
        }
      ],
      "url": "/schema/completion-config.en-US.json"
    }
  ]
}
