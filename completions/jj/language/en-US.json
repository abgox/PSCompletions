  "root": [
    {
      "name": "abandon",
      "tip": [
        "U: abandon [OPTIONS] [REVSETS]...\n",
        "Abandon a revision"
      ],
      "options": [
        {
          "name": "--retain-bookmarks",
          "tip": [
            "Do not delete bookmarks pointing to the revisions to abandon"
          ]
        },
        {
          "name": "--restore-descendants",
          "tip": [
            "Do not modify the content of the children of the abandoned commits"
          ]
        }
      ]
    },
    {
      "name": "absorb",
      "tip": [
        "U: absorb [OPTIONS] [FILESETS]...\n",
        "Move changes from a revision into the stack of mutable revisions"
      ],
      "options": [
        {
          "name": "-f",
          "alias": [
            "--from"
          ],
          "tip": [
            "U: -f, --from <REVSET>\n",
            "Source revision to absorb from [default: @]"
          ]
        },
        {
          "name": "-t",
          "alias": [
            "--into"
          ],
          "tip": [
            "U: -t, --into <REVSETS>\n",
            "Destination revisions to absorb into [default: mutable()]"
          ]
        }
      ]
    },
    {
      "name": "bisect",
      "tip": [
        "Find a bad revision by bisection"
      ],
      "next": [
        {
          "name": "run",
          "tip": [
            "U: run [OPTIONS] --range <REVSETS> [COMMAND] [ARGS]...\n",
