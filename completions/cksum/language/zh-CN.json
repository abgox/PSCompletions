{
  "meta": {
    "url": "https://github.com/uutils/coreutils",
    "description": [
      "Print CRC and size for each file.",
      "Completion was written based on [uutils/coreutils](https://github.com/uutils/coreutils)."
    ]
  },
  "options": [
    {
      "name": "--raw",
      "tip": [
        "emit a raw binary digest, not hexadecimal"
      ]
    },
    {
      "name": "--tag",
      "tip": [
        "create a BSD style checksum, undo --untagged (default)"
      ]
    },
    {
      "name": "--untagged",
      "tip": [
        "create a reversed style checksum, without digest type"
      ]
    },
    {
      "name": "--version",
      "alias": [
        "-V"
      ],
      "tip": [
        "显示版本信息。"
      ]
    },
    {
      "name": "-a",
      "alias": [
        "--algorithm"
      ],
      "tip": [
        "U: -a, --algorithm <ALGORITHM>",
        "select the digest type to use. See DIGEST below",
        "[possible values: sysv, bsd, crc, md5, sha1, sha224, sha256, sha384, sha512, blake2b, sm3]"
      ],
      "next": [
        {
          "name": "blake2b"
        },
        {
          "name": "bsd"
        },
        {
          "name": "crc"
        },
        {
          "name": "md5"
        },
        {
          "name": "sha1"
        },
        {
          "name": "sha224"
        },
        {
          "name": "sha256"
        },
        {
          "name": "sha384"
        },
        {
          "name": "sha512"
        },
        {
          "name": "sm3"
        },
        {
          "name": "sysv"
        }
      ]
    },
    {
      "name": "-b",
      "alias": [
        "--base64"
      ],
      "tip": [
        "emit a base64 digest, not hexadecimal"
      ]
    },
    {
      "name": "-l",
      "alias": [
        "--length"
      ],
      "tip": [
        "U: -l, --length <LENGTH>",
        "digest length in bits;",
        "must not exceed the max for the blake2 algorithm and must be a multiple of 8"
      ],
      "next": 0
    }
  ],
  "common_options": [
    {
      "name": "--help",
      "alias": [
        "-h"
      ],
      "tip": [
        "显示帮助信息。"
      ]
    }
  ]
}
