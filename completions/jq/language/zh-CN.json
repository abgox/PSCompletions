{
  "meta": {
    "url": "https://jqlang.org/",
    "description": [
      "一款轻量且灵活的命令行 JSON 处理器。"
    ]
  },
  "options": [
    {
      "name": "--version",
      "tip": [
        "显示版本信息。"
      ]
    }
  ],
  "common_options": [
    {
      "name": "--",
      "tip": [
        "终止参数处理；"
      ]
    },
    {
      "name": "--arg",
      "tip": [
        "U: --arg <name> <value>",
        "将 $name 设置为字符串值；"
      ],
      "next": 0
    },
    {
      "name": "--argjson",
      "tip": [
        "U: --argjson <name> <value>",
        "将 $name 设置为 JSON 值；"
      ],
      "next": 0
    },
    {
      "name": "--args",
      "tip": [
        "将剩余参数作为位置字符串值；"
      ],
      "next": [
        {
          "name": "."
        }
      ]
    },
    {
      "name": "--build-configuration",
      "tip": [
        "显示 jq 的构建配置；"
      ]
    },
    {
      "name": "--help",
      "tip": [
        "显示帮助信息。"
      ]
    },
    {
      "name": "--indent",
      "tip": [
        "U: --indent <n>",
        "使用 n 个空格缩进（最大 7 个空格）"
      ],
      "next": [
        {
          "name": "1"
        },
        {
          "name": "2"
        },
        {
          "name": "3"
        },
        {
          "name": "4"
        },
        {
          "name": "5"
        },
        {
          "name": "6"
        },
        {
          "name": "7"
        }
      ]
    },
    {
      "name": "--jsonargs",
      "tip": [
        "将剩余参数作为位置 JSON 值；"
      ],
      "next": [
        {
          "name": "."
        }
      ]
    },
    {
      "name": "--raw-output0",
      "tip": [
        "隐含 -r 选项，每个输出后输出 NUL 字符；"
      ]
    },
    {
      "name": "--rawfile",
      "tip": [
        "U: --rawfile <name> <file>",
        "将 $name 设置为文件的字符串内容；"
      ],
      "next": 0
    },
    {
      "name": "--seq",
      "tip": [
        "以 application/json-seq 格式解析输入/输出；"
      ]
    },
    {
      "name": "--slurpfile",
      "tip": [
        "U: --slurpfile <name> <file>",
        "将 $name 设置为从文件读取的 JSON 值数组；"
      ],
      "next": 0
    },
    {
      "name": "--stream",
      "tip": [
        "以流式方式解析输入值；"
      ]
    },
    {
      "name": "--stream-errors",
      "tip": [
        "隐含 --stream 选项，将解析错误报告为数组；"
      ]
    },
    {
      "name": "--tab",
      "tip": [
        "使用制表符缩进；"
      ]
    },
    {
      "name": "--unbuffered",
      "tip": [
        "在每个输出后刷新输出流；"
      ]
    },
    {
      "name": "-a",
      "alias": [
        "--ascii-output"
      ],
      "tip": [
        "U: -a, --ascii-output",
        "仅使用转义序列输出 ASCII 字符的字符串；"
      ]
    },
    {
      "name": "-b",
      "alias": [
        "--binary"
      ],
      "tip": [
        "U: -b, --binary",
        "以二进制模式打开输入/输出流；"
      ]
    },
    {
      "name": "-c",
      "alias": [
        "--compact-output"
      ],
      "tip": [
        "U: -c, --compact-output",
        "紧凑输出而不是打印更好看的输出；"
      ]
    },
    {
      "name": "-C",
      "alias": [
        "--color-output"
      ],
      "tip": [
        "U: -C, --color-output",
        "对 JSON 输出进行颜色化；"
      ]
    },
    {
      "name": "-e",
      "alias": [
        "--exit-status"
      ],
      "tip": [
        "U: -e, --exit-status",
        "根据输出设置退出状态码；"
      ]
    },
    {
      "name": "-f",
      "alias": [
        "--from-file"
      ],
      "tip": [
        "U: -f, --from-file <file>",
        "从文件加载过滤器；"
      ],
      "next": 0
    },
    {
      "name": "-j",
      "alias": [
        "--join-output"
      ],
      "tip": [
        "U: -j, --join-output",
        "隐含 -r 选项，每个输出后输出不带换行符的字符串；"
      ]
    },
    {
      "name": "-L",
      "alias": [
        "--library-path"
      ],
      "tip": [
        "U: -L, --library-path <dir>",
        "从目录搜索模块；"
      ],
      "next": 0
    },
    {
      "name": "-M",
      "alias": [
        "--monochrome-output"
      ],
      "tip": [
        "U: -M, --monochrome-output",
        "禁用颜色化输出；"
      ]
    },
    {
      "name": "-n",
      "alias": [
        "--null-input"
      ],
      "tip": [
        "U: -n, --null-input",
        "将 `null` 作为单输入值。"
      ]
    },
    {
      "name": "-r",
      "alias": [
        "--raw-output"
      ],
      "tip": [
        "U: -r, --raw-output",
        "输出字符串而不包含转义字符和引号；"
      ]
    },
    {
      "name": "-R",
      "alias": [
        "--raw-input"
      ],
      "tip": [
        "U: -R, --raw-input",
        "将每行作为字符串而不是 JSON 读取；"
      ]
    },
    {
      "name": "-s",
      "alias": [
        "--slurp"
      ],
      "tip": [
        "U: -s, --slurp",
        "将所有输入值合并为一个数组，并将其作为单输入值；"
      ]
    },
    {
      "name": "-S",
      "alias": [
        "--sort-keys"
      ],
      "tip": [
        "U: -S, --sort-keys",
        "在输出时对每个对象的键进行排序；"
      ]
    }
  ]
}
