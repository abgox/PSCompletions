{
  "meta": {
    "url": "https://jqlang.org/",
    "description": [
      "A lightweight and flexible command-line JSON processor."
    ]
  },
  "options": [
    {
      "name": "--version",
      "tip": [
        "Show the version."
      ]
    }
  ],
  "common_options": [
    {
      "name": "--",
      "tip": [
        "Terminates argument processing;"
      ]
    },
    {
      "name": "--arg",
      "tip": [
        "U: --arg <name> <value>",
        "Set $name to the string value;"
      ],
      "next": 0
    },
    {
      "name": "--argjson",
      "tip": [
        "U: --argjson <name> <value>",
        "Set $name to the JSON value;"
      ],
      "next": 0
    },
    {
      "name": "--args",
      "tip": [
        "Consume remaining arguments as positional string values;"
      ],
      "next": [
        {
          "name": "."
        }
      ]
    },
    {
      "name": "--build-configuration",
      "tip": [
        "Show jq's build configuration;"
      ]
    },
    {
      "name": "--help",
      "tip": [
        "Show the help."
      ]
    },
    {
      "name": "--indent",
      "tip": [
        "U: --indent <n>",
        "Use n spaces for indentation (max 7 spaces);"
      ],
      "next": [
        {
          "name": "1"
        },
        {
          "name": "2"
        },
        {
          "name": "3"
        },
        {
          "name": "4"
        },
        {
          "name": "5"
        },
        {
          "name": "6"
        },
        {
          "name": "7"
        }
      ]
    },
    {
      "name": "--jsonargs",
      "tip": [
        "Consume remaining arguments as positional JSON values;"
      ],
      "next": [
        {
          "name": "."
        }
      ]
    },
    {
      "name": "--raw-output0",
      "tip": [
        "Implies -r and output NUL after each output;"
      ]
    },
    {
      "name": "--rawfile",
      "tip": [
        "U: --rawfile <name> <file>",
        "Set $name to string contents of file;"
      ],
      "next": 0
    },
    {
      "name": "--seq",
      "tip": [
        "Parse input/output as application/json-seq;"
      ]
    },
    {
      "name": "--slurpfile",
      "tip": [
        "U: --slurpfile <name> <file>",
        "Set $name to an array of JSON values read from the file;"
      ],
      "next": 0
    },
    {
      "name": "--stream",
      "tip": [
        "Parse the input value in streaming fashion;"
      ]
    },
    {
      "name": "--stream-errors",
      "tip": [
        "Implies --stream and report parse error as an array;"
      ]
    },
    {
      "name": "--tab",
      "tip": [
        "Use tabs for indentation;"
      ]
    },
    {
      "name": "--unbuffered",
      "tip": [
        "Flush output stream after each output;"
      ]
    },
    {
      "name": "-a",
      "alias": [
        "--ascii-output"
      ],
      "tip": [
        "U: -a, --ascii-output",
        "Output strings by only ASCII characters using escape sequences;"
      ]
    },
    {
      "name": "-b",
      "alias": [
        "--binary"
      ],
      "tip": [
        "U: -b, --binary",
        "Open input/output streams in binary mode;"
      ]
    },
    {
      "name": "-c",
      "alias": [
        "--compact-output"
      ],
      "tip": [
        "U: -c, --compact-output",
        "Compact instead of pretty-printed output;"
      ]
    },
    {
      "name": "-C",
      "alias": [
        "--color-output"
      ],
      "tip": [
        "U: -C, --color-output",
        "Colorize JSON output;"
      ]
    },
    {
      "name": "-e",
      "alias": [
        "--exit-status"
      ],
      "tip": [
        "U: -e, --exit-status",
        "Set exit status code based on the output;"
      ]
    },
    {
      "name": "-f",
      "alias": [
        "--from-file"
      ],
      "tip": [
        "U: -f, --from-file <file>",
        "Load the filter from a file;"
      ],
      "next": 0
    },
    {
      "name": "-j",
      "alias": [
        "--join-output"
      ],
      "tip": [
        "U: -j, --join-output",
        "Implies -r and output without newline after each output;"
      ]
    },
    {
      "name": "-L",
      "alias": [
        "--library-path"
      ],
      "tip": [
        "U: -L, --library-path <dir>",
        "Search modules from the directory;"
      ],
      "next": 0
    },
    {
      "name": "-M",
      "alias": [
        "--monochrome-output"
      ],
      "tip": [
        "U: -M, --monochrome-output",
        "Disable colored output;"
      ]
    },
    {
      "name": "-n",
      "alias": [
        "--null-input"
      ],
      "tip": [
        "U: -n, --null-input",
        "Use `null` as the single input value."
      ]
    },
    {
      "name": "-r",
      "alias": [
        "--raw-output"
      ],
      "tip": [
        "U: -r, --raw-output",
        "Output strings without escapes and quotes;"
      ]
    },
    {
      "name": "-R",
      "alias": [
        "--raw-input"
      ],
      "tip": [
        "U: -R, --raw-input",
        "Read each line as string instead of JSON;"
      ]
    },
    {
      "name": "-s",
      "alias": [
        "--slurp"
      ],
      "tip": [
        "U: -s, --slurp",
        "Read all inputs into an array and use it as the single input value;"
      ]
    },
    {
      "name": "-S",
      "alias": [
        "--sort-keys"
      ],
      "tip": [
        "U: -S, --sort-keys",
        "Sort keys of each object on output;"
      ]
    }
  ]
}
