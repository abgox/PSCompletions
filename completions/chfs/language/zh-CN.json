{
  "meta": {
    "url": "http://iscute.cn/chfs",
    "description": [
      "CuteHttpFileServer - 一个免费的、HTTP协议的文件共享服务器。"
    ]
  },
  "options": [
    {
      "name": "--path=",
      "tip": [
        "指定存储共享文件的目录。",
        "E: --path=D:\\",
        "   --path=\"C:\\Program Files\""
      ]
    },
    {
      "name": "--name=",
      "tip": [
        "HTTP 监听端口(默认为80)。",
        "E: --port=8080"
      ]
    },
    {
      "name": "--file=",
      "tip": [
        "指定一个配置文件去覆盖设置。",
        "E: --file=\"config.json\""
      ]
    },
    {
      "name": "--version",
      "tip": [
        "显示当前版本。"
      ]
    }
  ],
  "common_options": [
    {
      "name": "--help",
      "alias": [
        "--help-long",
        "--help-man"
      ],
      "tip": [
        "显示上下文相关帮助。"
      ]
    }
  ]
}
