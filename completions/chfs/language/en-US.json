{
  "meta": {
    "url": "http://iscute.cn/chfs",
    "description": [
      "CuteHttpFileServer - A free, HTTP protocol file sharing server cross-platform file sharing server."
    ]
  },
  "options": [
    {
      "name": "--path=",
      "tip": [
        "Directories where store shared files, separated by '|'. (default \".\")",
        "E: --path=D:\\",
        "   --path=\"C:\\Program Files\""
      ]
    },
    {
      "name": "--name=",
      "tip": [
        "HTTP listening port. (Default 80)",
        "E: --port=8080"
      ]
    },
    {
      "name": "--file=",
      "tip": [
        "A configuration file which overwrites the settings.",
        "E: --file=\"config.json\""
      ]
    },
    {
      "name": "--version",
      "tip": [
        "Show application version."
      ]
    }
  ],
  "common_options": [
    {
      "name": "--help",
      "alias": [
        "--help-long",
        "--help-man"
      ],
      "tip": [
        "Show context-sensitive help."
      ]
    }
  ]
}
