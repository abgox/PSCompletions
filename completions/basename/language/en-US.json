{
  "options": [
    {
      "name": "-a",
      "alias": ["--multiple"],
      "tip": "support multiple arguments and treat each as a NAME"
    },
    {
      "name": "-s",
      "alias": ["--suffix"],
      "symbol": "WriteSpaceTab",
      "tip": [
        "U: -s|--suffix <SUFFIX>\n",
        "remove a trailing SUFFIX; implies -a"
      ]
    },
    {
      "name": "-z",
      "alias": ["--zero"],
      "tip": "end each output line with NUL, not newline"
    },
    {
      "name": "--version",
      "alias": ["-V"],
      "tip": "Show the version."
    }
  ],
  "common_options": [
    {
      "name": "--help",
      "alias": ["-h"],
      "tip": "Show help."
    }
  ],
  "info": {
    "completion_info": {
      "url": "https://github.com/uutils/coreutils",
      "description": [
        "Print NAME with any leading directory components removed.\n",
        "Come from [uutils/coreutils](https://github.com/uutils/coreutils)"
      ]
    }
  }
}
