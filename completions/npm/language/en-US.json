{
  "meta": {
    "url": "https://www.npmjs.com/",
    "description": [
      "npm - package manager."
    ]
  },
  "root": [
    {
      "name": "cache",
      "tip": [
        "Manipulates packages cache."
      ],
      "next": [
        {
          "name": "add",
          "tip": [
            "Add a package to the local cache."
          ]
        },
        {
          "name": "clean",
          "tip": [
            "Clean packages cache."
          ]
        },
        {
          "name": "ls",
          "tip": [
            "List packages in the cache."
          ]
        },
        {
          "name": "verify",
          "tip": [
            "Verify the contents of the cache."
          ]
        }
      ]
    },
    {
      "name": "config",
      "tip": [
        "Manage the npm configuration."
      ],
      "next": [
        {
          "name": "delete",
          "tip": [
            "Delete a value from the npm config."
          ]
        },
        {
          "name": "edit",
          "tip": [
            "Open the npm config file in an editor."
          ]
        },
        {
          "name": "fix",
          "tip": [
            "Fix a broken package.json file."
          ]
        },
        {
          "name": "get",
          "tip": [
            "Get a value from the npm config."
          ]
        },
        {
          "name": "list",
          "tip": [
            "List the npm config."
          ]
        },
        {
          "name": "set",
          "tip": [
            "Set a value in the npm config."
          ]
        }
      ]
    },
    {
      "name": "help",
      "tip": [
        "U: help [<command>]",
        "Get help on npm.",
        "E: help install"
      ]
    },
    {
      "name": "init",
      "tip": [
        "Create a new package.json file."
      ],
      "options": [
        {
          "name": "-f",
          "alias": [
            "--force"
          ],
          "tip": [
            "U: -f, --force",
            "Overwrite package.json if it already exists."
          ]
        },
        {
          "name": "-y",
          "alias": [
            "--yes"
          ],
          "tip": [
            "U: -y, --yes",
            "Use default values for all options."
          ]
        }
      ]
    },
    {
      "name": "install",
      "alias": [
        "i"
      ],
      "tip": [
        "U: install, i <package>",
        "Install a package.",
        "E: install axios"
      ],
      "options": [
        {
          "name": "-D",
          "alias": [
            "--save-dev"
          ],
          "tip": [
            "U: -D, --save-dev",
            "Write to the devDependencies of package.json."
          ]
        },
        {
          "name": "-g",
          "alias": [
            "--global"
          ],
          "tip": [
            "U: -g, --global",
            "Install package globally."
          ]
        },
        {
          "name": "-S",
          "alias": [
            "--save"
          ],
          "tip": [
            "U: -S, --save",
            "Write to the dependencies of package.json."
          ]
        }
      ]
    },
    {
      "name": "list",
      "alias": [
        "ls"
      ],
      "tip": [
        "U: list|ls",
        "List installed packages."
      ],
      "options": [
        {
          "name": "-g",
          "alias": [
            "--global"
          ],
          "tip": [
            "U: -g, --global",
            "List globally installed packages."
          ]
        }
      ]
    },
    {
      "name": "outdated",
      "tip": [
        "Check for outdated packages."
      ]
    },
    {
      "name": "root",
      "tip": [
        "Display the effective npm root."
      ]
    },
    {
      "name": "run",
      "tip": [
        "U: run <script>",
        "Run a script defined in package.json.",
        "E: run build"
      ],
      "next": []
    },
    {
      "name": "search",
      "tip": [
        "U: search <query>",
        "Search for packages on npm.",
        "E: search vue"
      ]
    },
    {
      "name": "test",
      "tip": [
        "run this projects tests."
      ]
    },
    {
      "name": "uninstall",
      "tip": [
        "U: uninstall <package>",
        "Uninstall a package.",
        "E: uninstall express"
      ],
      "options": [
        {
          "name": "-D",
          "alias": [
            "--save-dev"
          ],
          "tip": [
            "U: -D, --save-dev",
            "Remove from the devDependencies of package.json."
          ]
        },
        {
          "name": "-g",
          "alias": [
            "--global"
          ],
          "tip": [
            "U: -g, --global",
            "Uninstall package globally."
          ]
        },
        {
          "name": "-S",
          "alias": [
            "--save"
          ],
          "tip": [
            "U: -S, --save",
            "Remove from the dependencies of package.json."
          ]
        }
      ]
    },
    {
      "name": "update",
      "tip": [
        "U: update <package>",
        "Update a package.",
        "E: update express"
      ],
      "options": [
        {
          "name": "-D",
          "alias": [
            "--save-dev"
          ],
          "tip": [
            "U: -D, --save-dev",
            "Update the devDependencies of package.json."
          ]
        },
        {
          "name": "-g",
          "alias": [
            "--global"
          ],
          "tip": [
            "U: -g, --global",
            "Update package globally."
          ]
        },
        {
          "name": "-S",
          "alias": [
            "--save"
          ],
          "tip": [
            "U: -S, --save",
            "Update the dependencies of package.json."
          ]
        }
      ]
    },
    {
      "name": "view",
      "tip": [
        "View registry information."
      ]
    }
  ],
  "options": [
    {
      "name": "-l",
      "tip": [
        "display usage info for all commands."
      ]
    },
    {
      "name": "-v",
      "alias": [
        "--version"
      ],
      "tip": [
        "U: -v, --version",
        "output the version number."
      ]
    }
  ],
  "common_options": [
    {
      "name": "-h",
      "alias": [
        "--help"
      ],
      "tip": [
        "U: -h, --help",
        "Show the help."
      ]
    }
  ]
}
