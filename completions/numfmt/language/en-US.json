{
  "meta": {
    "url": "https://github.com/uutils/coreutils",
    "description": [
      "Convert numbers from/to human-readable strings.",
      "Completion was written based on [uutils/coreutils](https://github.com/uutils/coreutils)."
    ]
  },
  "options": [
    {
      "name": "--field",
      "tip": [
        "U: --field <FIELDS>",
        "replace the numbers in these input fields; see FIELDS below [default: 1]"
      ],
      "next": 0
    },
    {
      "name": "--format",
      "tip": [
        "U: --format <FORMAT>",
        "use printf style floating-point FORMAT; see FORMAT below for details"
      ],
      "next": 0
    },
    {
      "name": "--from",
      "tip": [
        "U: --from <UNIT>",
        "auto-scale input numbers to UNITs; see UNIT below [default: none]"
      ],
      "next": [
        {
          "name": "auto",
          "tip": [
            "accept optional single/two letter suffix:",
            "1K = 1000, 1Ki = 1024, 1M = 1000000, 1Mi = 1048576"
          ]
        },
        {
          "name": "iec",
          "tip": [
            "accept optional single letter suffix:",
            "1K = 1024, 1M = 1048576, ..."
          ]
        },
        {
          "name": "iec-i",
          "tip": [
            "accept optional two-letter suffix:",
            "1Ki = 1024, 1Mi = 1048576, ..."
          ]
        },
        {
          "name": "none",
          "tip": [
            "no auto-scaling is done; suffixes will trigger an error"
          ]
        },
        {
          "name": "si",
          "tip": [
            "accept optional single letter suffix:",
            "1K = 1000, 1M = 1000000, ..."
          ]
        }
      ]
    },
    {
      "name": "--from-unit",
      "tip": [
        "U: --from-unit <N>",
        "specify the input unit size [default: 1]"
      ],
      "next": 0
    },
    {
      "name": "--header",
      "tip": [
        "U: --header [<N>]",
        "print (without converting) the first N header lines; N defaults to 1 if not specified"
      ],
      "next": 0
    },
    {
      "name": "--invalid",
      "tip": [
        "U: --invalid <INVALID>",
        "set the failure mode for invalid input [default: abort]",
        "[possible values: abort, fail, warn, ignore]"
      ],
      "next": [
        {
          "name": "abort"
        },
        {
          "name": "fail"
        },
        {
          "name": "ignore"
        },
        {
          "name": "warn"
        }
      ]
    },
    {
      "name": "--padding",
      "tip": [
        "U: --padding <N>",
        "pad the output to N characters;",
        "positive N will right-align; negative N will left-align;",
        "padding is ignored if the output is wider than N;",
        "the default is to automatically pad if a whitespace is found"
      ],
      "next": 0
    },
    {
      "name": "--round",
      "tip": [
        "U: --round <METHOD>",
        "use METHOD for rounding when scaling [default: from-zero]",
        "[possible values: up, down, from-zero, towards-zero, nearest]"
      ],
      "next": [
        {
          "name": "down"
        },
        {
          "name": "from-zero"
        },
        {
          "name": "nearest"
        },
        {
          "name": "towards-zero"
        },
        {
          "name": "up"
        }
      ]
    },
    {
      "name": "--suffix",
      "tip": [
        "U: --suffix <SUFFIX>",
        "print SUFFIX after each formatted number, and accept inputs optionally ending with SUFFIX"
      ],
      "next": 0
    },
    {
      "name": "--to",
      "tip": [
        "U: --to <UNIT>",
        "auto-scale output numbers to UNITs; see UNIT below [default: none]"
      ],
      "next": [
        {
          "name": "auto",
          "tip": [
            "accept optional single/two letter suffix:",
            "1K = 1000, 1Ki = 1024, 1M = 1000000, 1Mi = 1048576"
          ]
        },
        {
          "name": "iec",
          "tip": [
            "accept optional single letter suffix:",
            "1K = 1024, 1M = 1048576, ..."
          ]
        },
        {
          "name": "iec-i",
          "tip": [
            "accept optional two-letter suffix:",
            "1Ki = 1024, 1Mi = 1048576, ..."
          ]
        },
        {
          "name": "none",
          "tip": [
            "no auto-scaling is done; suffixes will trigger an error"
          ]
        },
        {
          "name": "si",
          "tip": [
            "accept optional single letter suffix:",
            "1K = 1000, 1M = 1000000, ..."
          ]
        }
      ]
    },
    {
      "name": "--to-unit",
      "tip": [
        "U: --to-unit <N> ",
        "the output unit size [default: 1]"
      ],
      "next": 0
    },
    {
      "name": "--version",
      "alias": [
        "-V"
      ],
      "tip": [
        "Show the version."
      ]
    },
    {
      "name": "-d",
      "alias": [
        "--delimiter"
      ],
      "tip": [
        "U: -d, --delimiter <X>",
        "use X instead of whitespace for field delimiter"
      ],
      "next": 0
    }
  ],
  "common_options": [
    {
      "name": "--help",
      "alias": [
        "-h"
      ],
      "tip": [
        "Show the help."
      ]
    }
  ]
}
