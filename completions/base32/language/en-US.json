{
  "options": [
    {
      "name": "-d",
      "alias": ["-decode"],
      "tip": "decode data"
    },
    {
      "name": "-i",
      "alias": ["-ignore-garbage"],
      "tip": "when decoding, ignore non-alphabetic characters"
    },
    {
      "name": "-w",
      "alias": ["-wrap"],
      "symbol": "WriteSpaceTab",
      "tip": [
