{
  "meta": {
    "url": "https://idursun.github.io/jjui/",
    "description": [
      "Jujutsu UI (jjui) is a Text User Interface (TUI) designed for interacting with the Jujutsu version control system."
    ]
  },
  "common_options": [
    {
      "name": "-config",
      "tip": [
        "Open configuration file in $EDITOR"
      ]
    },
    {
      "name": "-help",
      "tip": [
        "Show the help."
      ]
    },
    {
      "name": "-n",
      "alias": [
        "-limit"
      ],
      "tip": [
        "U: -n, -limit <int>",
        "Number of revisions to show (default: 0)"
      ]
    },
    {
      "name": "-p",
      "alias": [
        "-period"
      ],
      "tip": [
        "U: -p, -period <int>",
        "Override auto-refresh interval (seconds, set to 0 to disable) (default -1)"
      ]
    },
    {
      "name": "-r",
      "alias": [
        "-revset"
      ],
      "tip": [
        "U: -r, -revset <string>",
        "Set default revset"
      ]
    },
    {
      "name": "-version",
      "tip": [
        "Show the version."
      ]
    }
  ]
}
