{
  "meta": {
    "url": "https://github.com/Pana/nrm",
    "description": [
      "nrm - npm registry manager."
    ]
  },
  "root": [
    {
      "name": "ls",
      "tip": [
        "List all the registries."
      ]
    },
    {
      "name": "current",
      "tip": [
        "Show current registry name or URL."
      ],
      "options": [
        {
          "name": "-u",
          "tip": [
            "Show the registry URL instead of the name."
          ]
        }
      ]
    },
    {
      "name": "use",
      "tip": [
        "U: use <name>",
        "Change current registry"
      ]
    },
    {
      "name": "add",
      "tip": [
        "U: add <name> <url> [home]",
        "Add custom registry"
      ]
    },
    {
      "name": "login",
      "tip": [
        "U: login [options] <name> [base64]",
        "Set authorize information for a custom registry with a base64 encoded string or username and password."
      ]
    },
    {
      "name": "set-hosted-repo",
      "tip": [
        "U: set-hosted-repo <name> <repo>",
        "Set hosted npm repository for a custom registry to publish package."
      ]
    },
    {
      "name": "set-scope",
      "tip": [
        "U: set-scope <scopeName> <url>",
        "Associating a scope with a registry."
      ]
    },
    {
      "name": "del-scope",
      "tip": [
        "U: del-scope <scopeName>",
        "Remove a scope."
      ]
    },
    {
      "name": "set",
      "tip": [
        "U: set [options] <name>",
        "Set a custom registry attribute."
      ]
    },
    {
      "name": "rename",
      "tip": [
        "U: rename <name> <newName>",
        "Change custom registry name."
      ]
    },
    {
      "name": "del",
      "tip": [
        "U: del <name>",
        "Delete custom registry."
      ]
    },
    {
      "name": "home",
      "tip": [
        "U: home <name> [browser]",
        "Open the homepage of registry with optional browser."
      ]
    },
    {
      "name": "test",
      "tip": [
        "U: test [registry]",
        "Show response time for specific or all registries."
      ]
    },
    {
      "name": "help",
      "tip": [
        "U: help [command]",
        "display help for command."
      ]
    }
  ],
  "options": [
    {
      "name": "-V",
      "tip": [
        "Show the version of nrm."
      ]
    }
  ],
  "common_options": [
    {
      "name": "-h",
      "alias": [
        "--help"
      ],
      "tip": [
        "U: -h, --help",
        "Show the help."
      ]
    }
  ]
}
