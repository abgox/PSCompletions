{
  "root": [
    {
      "name": "ls",
      "tip": "List all the registries."
    },
    {
      "name": "current",
      "tip": "Show current registry name or URL.",
      "options": [
        {
          "name": "-u",
          "tip": "Show the registry URL instead of the name."
        }
      ]
    },
    {
      "name": "use",
      "tip": [
        "U: use <name>\n",
        "Change current registry"
      ]
    },
    {
      "name": "add",
      "tip": [
        "U: add <name> <url> [home]\n",
        "Add custom registry"
      ]
    },
    {
      "name": "login",
      "tip": [
        "U: login [options] <name> [base64]\n",
