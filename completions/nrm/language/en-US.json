    },
    {
      "name": "test",
      "tip": [
        "U: test [registry]\n",
        "Show response time for specific or all registries."
      ]
    },
    {
      "name": "help",
      "tip": [
        "U: help [command]\n",
        "display help for command."
      ]
    }
